#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

   int row,col,i,j;
    long long a[100][100];
    scanf("%d %d",&row,&col);
    for(i=0;i<row;i++)
    {
        for(j=0;j<col;j++)
    {
        scanf("%lld",&a[i][j]);
    }
    }
    
  for(j=0;j<col;j++)
{int sum=0;
    for(i=0;i<row;i++)
    {
        sum=sum+a[i][j];
    }
    printf("%d\n",sum);
}
}
