#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main()
{
    int row,col,i,j;
    long a[100][100];
    scanf("%d %d",&row,&col);
    for(i=1;i<=row;i++)
    {
        for(j=1;j<=col;j++)
        {
            scanf("%ld",&a[i][j]);
        }
    }
    for(i=1;i<=row;i++)
    {int sum=0;
        for(j=1;j<=col;j++)
        {
            sum=sum+a[i][j];
        }
        printf("%d\n",sum);
    }
}
